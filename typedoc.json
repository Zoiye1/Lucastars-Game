{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src/api/src", "./src/shared", "./src/web/src"],
    "entryPointStrategy": "expand",
    "projectDocuments": ["./docs/index.md"],
    "out": "./dist/docs",
    "searchInDocuments": true,
    "searchInComments": true,
    "includeVersion": true,
    "validation": true,
    "readme": "none",
    "excludePrivate": false,
    "visibilityFilters": {
        "private": true,
        "protected": true,
        "inherited": true
    },
    "exclude": [
        "**/node_modules/**",
        "./src/api/src/game-base/playerSessionExpressMiddleware.ts"
    ],
    "navigationLinks": {
        "HBO-ICT Knowledgebase": "https://knowledgebase.hbo-ict-hva.nl/3_onderwijs/se/opdracht3/",
        "HBO-ICT.Cloud": "https://hbo-ict.cloud/"
    },
    //"plugin": ["typedoc-plugin-mermaid"],
    "ignoredHighlightLanguages": ["mermaid"],
    "customCss": "./docs/.typedoc/custom.css",
    "customJs": "./docs/.typedoc/custom.js"
}
